{
    "version": 2,
    "builds": [
      {
        "src": "client/build/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "server/api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/$1"
      }
    ]
  }
  